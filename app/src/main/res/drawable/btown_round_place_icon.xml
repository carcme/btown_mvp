<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:shape="oval">
            <solid android:color="@color/md_yellow_600" />
        </shape>
    </item>
    <item>
        <shape
            android:shape="oval">
            <stroke android:width="0.5dp"
                android:color="@color/black"/>
        </shape>
    </item>
    <item>
        <rotate
            android:fromDegrees="0"
            android:toDegrees="0"
            android:pivotX="50%"
            android:pivotY="50%"
            android:drawable="@drawable/ic_place_color" />
    </item>

</layer-list>
